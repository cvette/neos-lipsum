{
    "name": "cvette/neos-lipsum",
    "type": "neos-plugin",
    "description": "This Neos plugin adds a 'Lorem Ipsum' node type.",
    "license": "GPL-3.0+",
	"keywords": [ "lorem", "ipsum", "neoscms", "flow" ],
	"authors": [
        {
            "name": "Christian Vette",
            "email": "neos@christianvette.de",
            "homepage": "https://www.christianvette.de",
            "role": "Developer"
        }
	],
	"require": {
		"neos/neos": "~4.0"
	},
    "replace": {
        "cvette/lipsum": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Vette\\Lipsum\\": "Classes"
        }
    },
    "extra": {
        "marketplace-account": "neos@christianvette.de",
        "installer-name": "Vette.Lipsum",
        "neos": {
            "package-key": "Vette.Lipsum"
        }
    }
}
